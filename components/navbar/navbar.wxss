.nav-wrap {
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  z-index: 1000;
  display: flex;
  align-items: center;
}

.nav-back,
.nav-logo {
  display: flex;
  align-items: center;
}

.back-pre {
  width: 48rpx;
  height: 48rpx;
  padding-left: 40rpx;
}
.logo {
  width: 160rpx;
  /*height: 40rpx;*/
  padding-left: 40rpx;
}

.title {
  position: absolute;
  /* top: 50%; */
  font-weight: 500;
  font-size: 32rpx;
  left: 50%;
  /* transform: translate(-50%, -50%); */
  transform: translateX(-50%);
}